<?xml version="1.0" encoding="utf-8"?>
<!-- drawable/color.xml -->
<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="200"
    android:viewportHeight="200"
    >
    <group>
        <clip-path
            android:pathData="M0 0H200V200H0V0Z"
            />
        <group
            android:translateX="-1"
            android:translateY="0"
            android:pivotX="100"
            android:pivotY="100"
            android:scaleX="2"
            android:scaleY="4"
            android:rotation="90"
            >
            <path
                android:pathData="M0 0V200H200V0"
                >
                <aapt:attr name="android:fillColor">
                    <gradient
                        android:type="linear"
                        android:startX="50"
                        android:startY="100"
                        android:endX="150"
                        android:endY="100"
                        >
                        <item
                            android:color="#001B1B1B"
                            android:offset="0"
                            />
                        <item
                            android:color="#1B1B1B"
                            android:offset="1"
                            />
                    </gradient>
                </aapt:attr>
            </path>
        </group>
    </group>
</vector>